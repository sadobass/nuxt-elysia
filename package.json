{
  "name": "nuxt-elysia",
  "version": "1.0.0-beta.1",
  "description": "Mount Elysia app in Nuxt",
  "repository": "tkesgar/nuxt-elysia",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "dev": "nuxi dev playground",
    "dev:bun": "bun --bun nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:build:bun": "NITRO_PRESET=bun nuxi build playground",
    "dev:start": "node playground/.output/server/index.mjs",
    "dev:start:bun": "bun playground/.output/server/index.mjs",
    "lint": "eslint .",
    "test": "vitest",
    "test:bun": "bun test bun",
    "test:types": "cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.1"
  },
  "peerDependencies": {
    "elysia": ">= 1.2",
    "@elysiajs/eden": ">= 1.2"
  },
  "devDependencies": {
    "@elysiajs/eden": "^1.4.5",
    "@elysiajs/node": "^1.4.2",
    "@elysiajs/swagger": "^1.3.1",
    "@nuxt/devtools": "^2.7.0",
    "@nuxt/eslint-config": "^1.10.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.1",
    "@nuxt/test-utils": "^3.20.1",
    "@types/bun": "^1.3.2",
    "@types/ejs": "^3.1.5",
    "cheerio": "^1.1.2",
    "ejs": "^3.1.10",
    "elysia": "^1.4.16",
    "eslint": "^9.39.1",
    "nuxt": "^4.2.1",
    "typescript": "~5.9.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.4"
  },
  "workspaces": ["playground", "samples/*"],
  "unbuild": {
    "rollup": {
      "inlineDependencies": [
        "ejs"
      ]
    }
  }
}
